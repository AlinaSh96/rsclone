{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "Phaser": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "import/no-unresolved": 0,
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "object-curly-spacing": [
      "error",
      "never"
    ]
  }
}